driver = mysql
connect = host=127.0.0.1 dbname=mailinabox user=root password=newpassword
default_pass_scheme = SSHA256
password_query = SELECT email as user, password FROM users WHERE email='%u';
user_query = SELECT email AS user, "mail" as uid, "mail" as gid, "/mail-data/user-data/mail/mailboxes/%d/%n" as home FROM users WHERE email='%u';
iterate_query = SELECT email AS user FROM users;
